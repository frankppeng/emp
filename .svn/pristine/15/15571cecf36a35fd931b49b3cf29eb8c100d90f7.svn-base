@charset "utf-8";
/* CSS Document */

body { margin:0; font:14px/1.5 "Hiragino Sans GB", "Microsoft YaHei", sans-serif,tahoma,arial,\5b8b\4f53; sans-serif; position:relative; color:#333; background:#e6e2dd url(img/bg.png) left 108px repeat-x; min-width: 1000px;}
.w{ width:1000px; margin:0 auto;}
.w1000{ width:1000px; margin:0 auto;}
.w_100{ width:100%;}
.light{ border-color:#ea3a3a !important;}
.greenLight{ border-color:#3aea48 !important;}
a:link,a:visited{color:#4E92DF}
a:hover,a:active{color:#CA4536}
.onlineService{width:96px; height:96px; position:fixed; z-index:1999; top:50%; left:50%; margin-left:510px;}
.onlineService a:link,.onlineService a:visited{color:#C00; text-decoration:underline; position:absolute; top:63px; left:39px; font-size:13px}
.onlineService a:hover{color:#F00}
.verticalScroll{overflow-x:hidden; overflow-y:auto;}

/* header */
.fixed .header { position:fixed; width:100%; top:0; left:0; z-index:999;}
.header { position:relative; width:100%; top:0; left:0; z-index:999;}
.topNav { height:36px; line-height:36px; background:#fff; position:relative; z-index:3; }
.topNav ul{color:#ccc}
.topNav li {display:inline-block; *display:inline; *zoom:1; padding:0 8px; text-align:center; position:relative;}
.topNav ul.fr li{display:inline-block; *display:inline; *zoom:1; border:1px solid #fff; padding:0 8px; border-width:0 1px; cursor:pointer;}
.topNav ul.fr li:hover{ border-color:#e8e8e8; box-shadow:0px 1px 12px 0 rgba(0, 0, 0, 0.2); z-index:1;}
.topNav ul.fr li .top_popup{ width:100%; position:absolute; display:none; top:36px; right:-1px; background:#fff; border:1px solid #e8e8e8; border-top:0; box-shadow:0px 2px 10px 0 rgba(0, 0, 0, 0.2); }
.topNav ul.fr li .top_popup li{ width:100%; padding:0; border:0; text-align:left; text-indent:10px; }
.topNav ul.fr li .top_popup li:hover{ box-shadow:none; background:#d83840; color:#fff;}
.topNav ul.fr li .top_popup li:hover>a{ color:#fff;}
.topNav ul.fr li .top_popup.top_popup_info{ width:280px;}
.topNav ul.fr li .top_popup.top_popup_info li{ text-indent:0; border-top:1px solid #e8e8e8;}
.topNav ul.fr li .top_popup.top_popup_info li>a{ width:234px; line-height:18px; padding:12px 22px 11px 24px; text-indent:0;}
.topNav ul.fr li .top_popup.top_popup_info li>a{ color:#999; position:relative;}
.topNav ul.fr li .top_popup.top_popup_info li:hover{ background:#fff; color:#999}
.topNav ul.fr li .top_popup.top_popup_info li.new>a{ color:#333;}
.topNav ul.fr li .top_popup.top_popup_info li.new>a:before{ width:6px; height:6px; color:#da3a44; font-size:50px; content:"·"; position:absolute; top:17px; left:9px;}
.topNav ul.fr li#welcome{min-width: 60px;}


.topNav ul.fr li .top_popup.top_popup_info li.more>a{ padding:6px 22px 6px 24px; text-align:center; color:#4e92df;}
.topNav,.topNav a { font-size:12px; color:#333;}
.topNav a {text-decoration:none; display:block; }
.topNav a:hover {color:#000;}
.topNav a .arrow_sub{width:0; height:0; border:solid; border-width:6px 5px 0 5px; border-color:#cdcdcd transparent transparent transparent; display:inline-block; *display:inline; *zoom:1; margin:15px 0 0 4px; position:relative; top:-1px; left:2px; }
.topNav .warn>a:before{ width:6px; height:6px; color:#da3a44; font-size:50px; content:"·"; position:absolute; top:-10px; right:10px;}
.triangleDownIcon{width:10px; height:12px; background-image:url(img/arrows.png); background-position:-10px -188px; display:inline-block; *display:inline; *zoom:1}
.userGuiderSelect{position:absolute; top:30px; left:-3px; background:#fff}

.mainNav { background:url(img/mainNav_bg.png) 0 0 repeat; z-index:1;}
.fixed .mainNav{position:relative;}
.mainNav a:link,.mainNav a:visited{color:#fff}
.sf-logo { width:160px; height:47px; position:relative; left:20px; top:12px; float:left; text-indent:-999px; background:url(img/sf-logo.png) 0 0 no-repeat; display:block;}
.mainMenu { float:right; }
.mainNav,.mainMenu { height:72px;}
.menu_li{ height:72px; line-height:72px; float:left; position:relative; z-index:2;}
.menu_a{ width:120px; height:68px; line-height:64px; padding-top:4px; font-size:16px; color:#fff; text-align:center; text-decoration:none; display:block;}
.menu_li.current .menu_a{ height:68px; line-height:64px; border-top:4px solid #ea3a3a; padding-top:0;}
.menu_a:hover{ background:#616162;}
	/* 导航展开层 */
.menu_sub{ width:242px; height:auto; position:absolute; left:-60px; top:69px; display:none; transition:display 10s linear; z-index:6;}
.menu_sub .arrow_sub{ height:13px; font:26px/26px "\5B8B\4F53"; color:#fff; position:absolute; left:50%; top:-12px; margin-left:-13px; overflow:hidden; cursor:default; z-index:3;}
.menu_sub .arrow_sub.gray{ color:#f5f5f5;}
.menu_sub .menu_item_cha{ width:240px; height:50px; line-height:50px; background:#f5f5f5; color:#000; border-radius:4px 4px 0 0; position:relative; cursor:default; font-size:14px;}
.menu_sub .menu_item_cha a{ color:#000; display:block;}
.menu_sub .menu_item_cha span{ width:19px; height:19px; background:url(img/menu_item_icon.png) -15px -266px no-repeat; float:left; margin:16px 11px 0 78px;}
.menu_sub .menu_item_cha p{ height:12px; font:12px/12px "\5B8B\4F53"; color:#999; position:absolute; top:19px; right:18px; text-indent:-6px; overflow:hidden; cursor:default;}
.menu_sub .menu_item_cha:hover a{ color:#d8373f;}
.menu_sub .menu_item_cha:hover a span{ background-position:-115px -266px;}
.menu_sub .menu_item_cha:hover p{ color:#d8373f;}
.menu_sub .menu_item{ width:240px; border-radius:4px; box-shadow:0 2px 8px rgba(0, 0, 0, 0.7); background:#fff; position:relative; z-index:6; border:1px solid #888;}
.menu_sub .menu_item1{ width:250px; height:350px; position:absolute; top:0; left:-240px; z-index:0;}
.menu_sub .menu_item1 dl{ padding:0 27px 0 19px;}
.menu_sub .menu_item1 dt{ line-height:18px; font-size:12px; color:#999; margin:6px 0 6px;}
.menu_sub .menu_item1 dd{ line-height:53px;}
.menu_sub .menu_item1 dd input{ width:182px;}
.menu_sub .menu_item1 dd .btn{ height:32px; line-height:32px; border-radius:4px; margin-top:10px; font-size:12px;}

	/* 图标 */
.menu_sub .menu_item_in{ width:240px; height:auto;}
.menu_sub .menu_item_in li{ width:50%; height:90px; float:left;}
.menu_sub .menu_item_in li a{ width:100%; height:90px; display:inline-block; *display:inline; *zoom:1; color:#000; text-decoration:none; position:relative;}
.menu_sub .menu_item_in li a p{ width:100%; height:30px; line-height:20px; text-align:center; overflow:hidden; font-weight:600; font-size:12px;}
.menu_sub .menu_item_in li a:hover{ color:#d8373f;}
.menu_sub .menu_item_in li.item_num a p{ }
.menu_sub .menu_item_in li.warn a span:after{ width:10px; height:10px; content:""; position:relative; background:#ea3a3a; top:-28px; display:inline-block; *display:inline; *zoom:1; border-radius:5px; box-shadow:1px 1px 4px rgba(0, 0, 0, 0.3);}
.menu_item_ico{ width:50px; height:50px; background:url(img/menu_item_icon.png) 0 0 no-repeat; margin:10px auto 0; display:block;}
.menu_item_ico0{ width:80px; background:none; font:40px/48px Arial, Helvetica, sans-serif; text-align:center;}
